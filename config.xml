<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vipinkhushu.push" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Culmyca 2016</name>
    <description>
       Elements Culmyca 2016 Android App
   </description>
   <author email="vipinkhushu@hotmail.com" href="http://www.vipinkhushu.com">
    Vipin Khushu
</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/assets/images/culmyca_logo.png" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="8000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
</widget>
